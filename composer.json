{
    "name": "smashed-egg/laravel-model-repository",
    "description": "Add support for Model Repositories to Laravel",
    "keywords": ["php", "laravel", "model", "repository"],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/smashed-egg/laravel-model-repository/issues",
        "source": "https://github.com/smashed-egg/laravel-model-repository"
    },
    "require": {
        "php": "^8.0.2",
        "illuminate/console": "^9.21|^10.0",
        "illuminate/contracts": "^9.21|^10.0",
        "illuminate/support": "^9.21|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.6|^8.0",
        "phpunit/phpunit": "^9.5"

    },
    "autoload": {
        "psr-4": {
            "SmashedEgg\\LaravelModelRepository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmashedEgg\\LaravelModelRepository\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tom Ellis"
        }
    ],
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "SmashedEgg\\LaravelModelRepository\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
